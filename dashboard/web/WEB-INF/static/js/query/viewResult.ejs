
<table class="table table-bordered table-hover">
<% for(var i = 0 ;i <data.length;i++){ %>
    <% if(i == 0){%>
        <tr style="font-weight: bold;               ">
            <% for(var j = 0 ;j < data[i].length; j++) { %>
                <td>
                    <%=data[i][j]%>
                </td>
            <% } %>
        </tr>
    <% }else{%>
        <tr>
            <% for(var j = 0 ;j < data[i].length; j++) { %>
                <td>
                    <%=data[i][j]%>
                </td>
            <% } %>
        </tr>
    <%}%>
<% } %>
</table>

