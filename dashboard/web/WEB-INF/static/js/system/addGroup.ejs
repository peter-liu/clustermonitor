<div class="modal">
    <div class="modal-header">
        <h3>新增Group</h3>
        <span id="showInfo" style="color: red"></span>
    </div>
    <div class="modal-body">
        <form class="form-horizontal" id="addGroupForm" style="padding: 0;">
            <input  type="hidden" name="id" id="id" value="<%=data.id?data.id:''%>">
            <table class="table table-bordered" >
                <tbody>
                    <tr>
                        <td style="border-top: none" colspan="2">
                            <div class="control-group" >
                                <label class="control-label" style="text-align: left" ><span style="color: red">*</span>Group Name：</label>
                                <div class="controls">
                                    <input type="text" name="name" value="<%=data.name?data.name:''%>" id="name" class="input-medium required" >
                                </div>
                            </div>
                        </td>

                    </tr>
                </tbody>
            </table>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" onclick="system.fn.addGroup('<%=data.randomDialogId%>');"   class="btn" id="save">确定</button>
        <button class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
    </div>
</div>
<style type="text/css">
    .form-horizontal .control-label {
        padding-top: 15px;
        width: 100px;
    }
    .form-horizontal .control-group {
        margin-bottom: 0;
    }
    .form-horizontal .controls {
        margin-left: 100px;
        padding: 10px 0;
    }
    .error{
        color: red;
    }
    .modal-header h3 {
         font-size: 20px;
        text-shadow: 0 1px 0 #ffffff;
    }
</style>

